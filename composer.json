{
  "name": "islem-kms/activable",
  "description": "A simple Content Moderation System for Laravel 5.* that allows you to Approve or Reject resources like posts, comments, users, etc.",
  "keywords": [
    "laravel",
    "status"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Islem Khemssi",
      "email": "khemissi.islem@gmail.com"
    }
  ],
  "autoload-dev": {
    "classmap": [
      "tests",
      "vendor/laravel/laravel/tests"
    ]
  },
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.0",
    "laravel/laravel": "5.*",
    "nunomaduro/collision": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "IslemKms\\Activable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "IslemKms\\Activable\\ActivationServiceProvider"
      ]
    }
  }
}
